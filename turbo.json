{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".output/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
